    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ActiveDebugProfile>https</ActiveDebugProfile>
  </PropertyGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="wwwroot\assets\images\logo\MixWarz_Green.png" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\assets\images\vito-placeholder-img\" />
